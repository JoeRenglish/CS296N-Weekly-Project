@model List<Story>

@{
    ViewData["Title"] = "Your Stories";
}

@if (Model.Count != 0)
{
    foreach (Story story in Model)
    {
        <p>
            @story.Title <br/>
            By @story.AppUser.UserName <br/>
            Published @story.Date <br/>
            <a asp-controller="Story" asp-action="Story" asp-route-id="@story.StoryId">Read Story</a>
        </p>
        <a asp-controller="Story" asp-action="DeleteStory" asp-route-id="@story.StoryId">Delete Story</a>
        <br/> <br/>
    }
}